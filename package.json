{
  "name": "ts-react-static",
  "private": true,
  "description": "My site build with React-Static",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "react-static build",
    "bundle": "react-static bundle",
    "export": "react-static export",
    "serve": "serve dist -p 3000 -s",
    "build-storybook": "build-storybook --quiet --loglevel=info",
    "storybook": "start-storybook -p 6006",
    "lint:ts": "eslint 'src/**/*.ts{,x}'",
    "lint:md": "markdownlint contents",
    "lint": "run-p -l lint:*",
    "test": "jest --maxWorkers=1",
    "screenshot": "zisui --serverCmd 'start-storybook -p 6006' --serverTimeout 100000 http://localhost:6006",
    "build:all": "yarn build && mkdir -p ./dist/storybook && yarn build-storybook -o ./dist/storybook"
  },
  "dependencies": {
    "@emotion/core": "10.0.17",
    "@emotion/styled": "10.0.17",
    "@material-ui/core": "4.4.2",
    "@material-ui/icons": "4.4.1",
    "chokidar": "3.0.2",
    "dayjs": "1.8.16",
    "emotion": "10.0.17",
    "emotion-theming": "10.0.18",
    "htmr": "0.8.6",
    "jdown": "1.0.3",
    "marked-sanitizer-github": "1.0.0",
    "node-emoji": "1.10.0",
    "prismjs": "1.17.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router-dom": "5.0.1",
    "react-scroll": "1.7.14",
    "react-static": "7.2.2",
    "react-static-plugin-mdx": "7.2.2",
    "react-static-plugin-react-router": "7.2.2",
    "react-static-plugin-sitemap": "7.2.2",
    "react-static-plugin-source-filesystem": "7.2.2",
    "react-static-plugin-typescript": "7.2.2",
    "react-use": "12.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.0",
    "@storybook/addon-actions": "5.2.1",
    "@storybook/addon-knobs": "5.2.1",
    "@storybook/addon-links": "5.2.1",
    "@storybook/addon-viewport": "5.2.1",
    "@storybook/addons": "5.2.1",
    "@storybook/cli": "5.2.1",
    "@storybook/react": "5.2.1",
    "@testing-library/react-hooks": "2.0.1",
    "@types/enzyme": "3.10.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/faker": "4.1.5",
    "@types/jest": "24.0.18",
    "@types/marked": "0.6.5",
    "@types/node": "12.7.5",
    "@types/node-emoji": "1.8.1",
    "@types/prismjs": "1.16.0",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-hot-loader": "4.1.0",
    "@types/react-router-dom": "4.3.5",
    "@types/react-scroll": "1.5.4",
    "@types/storybook__addon-actions": "3.4.3",
    "@types/storybook__addon-knobs": "5.0.4",
    "@types/storybook__addon-viewport": "4.1.1",
    "@types/storybook__react": "4.0.2",
    "@types/webpack-env": "1.14.0",
    "@typescript-eslint/eslint-plugin": "2.3.0",
    "@typescript-eslint/parser": "2.3.0",
    "babel-loader": "8.0.6",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "eslint": "6.4.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "2.0.1",
    "faker": "4.1.0",
    "husky": "3.0.5",
    "jest": "24.9.0",
    "jest-enzyme": "7.1.1",
    "lint-staged": "9.2.5",
    "markdownlint-cli": "0.18.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "serve": "11.1.0",
    "style-loader": "1.0.0",
    "ts-config-webpack-plugin": "1.4.0",
    "ts-jest": "24.1.0",
    "typescript": "3.6.3",
    "zisui": "1.3.4"
  },
  "resolutions": {
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/hoist-non-react-statics": "3.3.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest.setup.ts"
    ],
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/src/**/*.spec.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>config/tsconfig.test.json"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
