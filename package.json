{
  "name": "ts-react-static",
  "private": true,
  "description": "My site build with React-Static",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "react-static build",
    "bundle": "react-static bundle",
    "export": "react-static export",
    "serve": "serve dist -p 3000 -s",
    "build-storybook": "build-storybook --quiet --loglevel=info",
    "storybook": "start-storybook -p 6006",
    "lint:ts": "eslint 'src/**/*.ts{,x}'",
    "lint:md": "markdownlint contents",
    "lint": "run-p -l lint:*",
    "test": "jest --maxWorkers=1",
    "screenshot": "storycap --serverCmd 'start-storybook -p 6006' --serverTimeout 100000 http://localhost:6006 -i '**/regression'",
    "build:reg": "bash ./scripts/reg.sh",
    "build:all": "yarn build && yarn build:reg"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "chokidar": "3.5.3",
    "dayjs": "1.11.7",
    "emotion": "11.0.0",
    "htmr": "0.10.1",
    "jdown": "1.0.5",
    "marked-sanitizer-github": "1.0.1",
    "node-emoji": "1.11.0",
    "prismjs": "1.22.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-router-dom": "5.3.4",
    "react-scroll": "1.8.9",
    "react-static": "7.6.2",
    "react-static-plugin-mdx": "7.6.2",
    "react-static-plugin-react-router": "7.6.2",
    "react-static-plugin-sitemap": "7.6.2",
    "react-static-plugin-source-filesystem": "7.6.2",
    "react-static-plugin-typescript": "7.6.2",
    "react-use": "15.3.8"
  },
  "devDependencies": {
    "@babel/core": "7.13.13",
    "@storybook/addon-actions": "9.0.8",
    "@storybook/addon-knobs": "8.0.1",
    "@storybook/addon-links": "9.1.2",
    "@storybook/addon-viewport": "9.0.8",
    "@storybook/addons": "7.6.17",
    "@storybook/cli": "9.1.2",
    "@storybook/react": "9.1.2",
    "@testing-library/react-hooks": "8.0.1",
    "@types/enzyme": "3.10.8",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/faker": "6.6.11",
    "@types/jest": "30.0.0",
    "@types/marked": "6.0.0",
    "@types/node": "22.17.2",
    "@types/node-emoji": "2.1.0",
    "@types/prismjs": "1.16.4",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "@types/react-hot-loader": "4.1.1",
    "@types/react-router-dom": "5.1.7",
    "@types/react-scroll": "1.8.2",
    "@types/webpack-env": "1.16.0",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "8.39.1",
    "babel-loader": "10.0.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "5.2.0",
    "faker": "6.6.6",
    "husky": "9.1.7",
    "jest": "30.0.5",
    "jest-enzyme": "7.1.2",
    "lint-staged": "16.1.5",
    "markdownlint-cli": "0.27.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.6.2",
    "reg-cli": "0.17.0",
    "serve": "14.2.4",
    "storycap": "5.0.1",
    "style-loader": "4.0.0",
    "ts-config-webpack-plugin": "2.0.0",
    "ts-jest": "29.4.1",
    "typescript": "5.9.2"
  },
  "resolutions": {
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "@types/hoist-non-react-statics": "3.3.7"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest.setup.ts"
    ],
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/src/**/*.spec.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>config/tsconfig.test.json"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
