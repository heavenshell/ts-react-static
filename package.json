{
  "name": "ts-react-static",
  "private": true,
  "description": "My site build with React-Static",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "react-static build",
    "bundle": "react-static bundle",
    "export": "react-static export",
    "serve": "serve dist -p 3000 -s",
    "build-storybook": "build-storybook --quiet --loglevel=info",
    "storybook": "start-storybook -p 6006",
    "lint:ts": "eslint 'src/**/*.ts{,x}'",
    "lint:md": "markdownlint contents",
    "lint": "run-p -l lint:*",
    "test": "jest --maxWorkers=1",
    "screenshot": "storycap --serverCmd 'start-storybook -p 6006' --serverTimeout 100000 http://localhost:6006 -i '**/regression'",
    "build:reg": "bash ./scripts/reg.sh",
    "build:all": "yarn build && yarn build:reg"
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "chokidar": "3.4.1",
    "dayjs": "1.8.31",
    "emotion": "10.0.27",
    "emotion-theming": "10.0.27",
    "htmr": "0.8.8",
    "jdown": "1.0.4",
    "marked-sanitizer-github": "1.0.1",
    "node-emoji": "1.10.0",
    "prismjs": "1.20.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-scroll": "1.8.0",
    "react-static": "7.4.2",
    "react-static-plugin-mdx": "7.2.2",
    "react-static-plugin-react-router": "7.4.2",
    "react-static-plugin-sitemap": "7.4.2",
    "react-static-plugin-source-filesystem": "7.4.2",
    "react-static-plugin-typescript": "7.4.2",
    "react-use": "15.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.11.0",
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-knobs": "5.3.19",
    "@storybook/addon-links": "5.3.19",
    "@storybook/addon-viewport": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/cli": "5.3.19",
    "@storybook/react": "5.3.19",
    "@testing-library/react-hooks": "3.4.1",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/faker": "4.1.12",
    "@types/jest": "26.0.8",
    "@types/marked": "1.1.0",
    "@types/node": "12.12.53",
    "@types/node-emoji": "1.8.1",
    "@types/prismjs": "1.16.1",
    "@types/react": "16.9.44",
    "@types/react-dom": "16.9.8",
    "@types/react-hot-loader": "4.1.1",
    "@types/react-router-dom": "5.1.5",
    "@types/react-scroll": "1.5.5",
    "@types/webpack-env": "1.15.2",
    "@typescript-eslint/eslint-plugin": "3.7.1",
    "@typescript-eslint/parser": "3.7.1",
    "babel-loader": "8.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-react-hooks": "4.0.8",
    "faker": "4.1.0",
    "husky": "4.2.5",
    "jest": "26.2.2",
    "jest-enzyme": "7.1.2",
    "lint-staged": "10.2.11",
    "markdownlint-cli": "0.23.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "reg-cli": "0.16.1",
    "serve": "11.3.2",
    "storycap": "2.3.5",
    "style-loader": "1.2.1",
    "ts-config-webpack-plugin": "2.0.0",
    "ts-jest": "26.1.4",
    "typescript": "3.9.7"
  },
  "resolutions": {
    "@types/react": "16.9.44",
    "@types/react-dom": "16.9.8",
    "@types/hoist-non-react-statics": "3.3.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest.setup.ts"
    ],
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/src/**/*.spec.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>config/tsconfig.test.json"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
