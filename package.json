{
  "name": "ts-react-static",
  "private": true,
  "description": "My site build with React-Static",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "react-static build",
    "bundle": "react-static bundle",
    "export": "react-static export",
    "serve": "serve dist -p 3000 -s",
    "build-storybook": "build-storybook --quiet --loglevel=info",
    "storybook": "start-storybook -p 6006",
    "lint:ts": "eslint 'src/**/*.ts{,x}'",
    "lint:md": "markdownlint contents",
    "lint": "run-p -l lint:*",
    "test": "jest --maxWorkers=1",
    "screenshot": "storycap --serverCmd 'start-storybook -p 6006' --serverTimeout 100000 http://localhost:6006 -i '**/regression'",
    "build:reg": "bash ./scripts/reg.sh",
    "build:all": "yarn build && yarn build:reg"
  },
  "dependencies": {
    "@emotion/core": "10.1.1",
    "@emotion/styled": "10.0.27",
    "@material-ui/core": "4.11.2",
    "@material-ui/icons": "4.11.2",
    "chokidar": "3.4.3",
    "dayjs": "1.9.7",
    "emotion": "10.0.27",
    "emotion-theming": "10.0.27",
    "htmr": "0.9.2",
    "jdown": "1.0.4",
    "marked-sanitizer-github": "1.0.1",
    "node-emoji": "1.10.0",
    "prismjs": "1.22.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-router-dom": "5.2.0",
    "react-scroll": "1.8.1",
    "react-static": "7.4.2",
    "react-static-plugin-mdx": "7.2.2",
    "react-static-plugin-react-router": "7.4.2",
    "react-static-plugin-sitemap": "7.4.2",
    "react-static-plugin-source-filesystem": "7.4.2",
    "react-static-plugin-typescript": "7.4.2",
    "react-use": "15.3.4"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@storybook/addon-actions": "6.0.28",
    "@storybook/addon-knobs": "6.0.28",
    "@storybook/addon-links": "6.0.28",
    "@storybook/addon-viewport": "6.0.28",
    "@storybook/addons": "6.0.28",
    "@storybook/cli": "6.0.28",
    "@storybook/react": "6.0.28",
    "@testing-library/react-hooks": "3.4.2",
    "@types/enzyme": "3.10.8",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/faker": "5.1.4",
    "@types/jest": "26.0.15",
    "@types/marked": "1.1.0",
    "@types/node": "14.14.7",
    "@types/node-emoji": "1.8.1",
    "@types/prismjs": "1.16.2",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-hot-loader": "4.1.1",
    "@types/react-router-dom": "5.1.6",
    "@types/react-scroll": "1.8.2",
    "@types/webpack-env": "1.15.3",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "babel-loader": "8.2.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "eslint": "7.13.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "faker": "5.1.0",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "jest-enzyme": "7.1.2",
    "lint-staged": "10.5.1",
    "markdownlint-cli": "0.24.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "reg-cli": "0.16.2",
    "serve": "11.3.2",
    "storycap": "3.0.1",
    "style-loader": "2.0.0",
    "ts-config-webpack-plugin": "2.0.0",
    "ts-jest": "26.4.4",
    "typescript": "4.0.5"
  },
  "resolutions": {
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/hoist-non-react-statics": "3.3.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest.setup.ts"
    ],
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/src/**/*.spec.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>config/tsconfig.test.json"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
