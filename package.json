{
  "name": "ts-react-static",
  "private": true,
  "description": "My site build with React-Static",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "react-static build",
    "bundle": "react-static bundle",
    "export": "react-static export",
    "serve": "serve dist -p 3000 -s",
    "build-storybook": "build-storybook --quiet --loglevel=info",
    "storybook": "start-storybook -p 6006",
    "lint:ts": "eslint 'src/**/*.ts{,x}'",
    "lint:md": "markdownlint contents",
    "lint": "run-p -l lint:*",
    "test": "jest --maxWorkers=1",
    "screenshot": "storycap --serverCmd 'start-storybook -p 6006' --serverTimeout 100000 http://localhost:6006 -i '**/regression'",
    "build:reg": "bash ./scripts/reg.sh",
    "build:all": "yarn build && yarn build:reg"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "chokidar": "3.5.3",
    "dayjs": "1.11.7",
    "emotion": "11.0.0",
    "htmr": "0.10.1",
    "jdown": "1.0.5",
    "marked-sanitizer-github": "1.0.1",
    "node-emoji": "1.11.0",
    "prismjs": "1.22.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-router-dom": "5.3.4",
    "react-scroll": "1.8.9",
    "react-static": "7.6.2",
    "react-static-plugin-mdx": "7.6.2",
    "react-static-plugin-react-router": "7.6.2",
    "react-static-plugin-sitemap": "7.6.2",
    "react-static-plugin-source-filesystem": "7.6.2",
    "react-static-plugin-typescript": "7.6.2",
    "react-use": "15.3.8"
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-knobs": "6.4.0",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addon-viewport": "6.5.16",
    "@storybook/addons": "6.5.16",
    "@storybook/cli": "6.5.16",
    "@storybook/react": "6.5.16",
    "@testing-library/react-hooks": "4.0.1",
    "@types/enzyme": "3.10.18",
    "@types/enzyme-adapter-react-16": "1.0.9",
    "@types/faker": "5.5.9",
    "@types/jest": "26.0.24",
    "@types/marked": "1.2.2",
    "@types/node": "14.18.63",
    "@types/node-emoji": "1.8.2",
    "@types/prismjs": "1.26.3",
    "@types/react": "16.14.56",
    "@types/react-dom": "16.9.24",
    "@types/react-hot-loader": "4.1.1",
    "@types/react-router-dom": "5.3.3",
    "@types/react-scroll": "1.8.10",
    "@types/webpack-env": "1.18.4",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "babel-loader": "8.3.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.8",
    "eslint": "7.32.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "faker": "5.5.3",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-enzyme": "7.1.2",
    "lint-staged": "10.5.4",
    "markdownlint-cli": "0.39.0",
    "npm-run-all2": "5.0.0",
    "prettier": "2.8.8",
    "reg-cli": "0.18.0",
    "serve": "11.3.2",
    "storycap": "3.1.9",
    "style-loader": "2.0.0",
    "ts-config-webpack-plugin": "2.0.3",
    "ts-jest": "26.5.6",
    "typescript": "4.9.5"
  },
  "resolutions": {
    "@types/react": "16.14.56",
    "@types/react-dom": "16.9.24",
    "@types/hoist-non-react-statics": "3.3.5"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest.setup.ts"
    ],
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/src/**/*.spec.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>config/tsconfig.test.json"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
